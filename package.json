{
  "name": "judge0-ide",
  "version": "1.0.0",
  "description": "Judge0 IDE with Tailwind CSS",
  "main": "js/electron.js",
  "scripts": {
    "start": "electron .",
    "build:css": "tailwindcss -i ./css/tailwind.css -o ./css/tailwind.output.css --watch",
    "build": "tailwindcss -i ./css/tailwind.css -o ./css/tailwind.output.css --minify",
    "dev": "concurrently \"npm run build:css\" \"npm start\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
